pipeline {
    agent any
    tools {
        // Define the tool name and version
        maven 'Maven 3.9.4'
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Build') {
            steps {
                script {
                    // Use the configured Maven installation
                    bat "mvn clean package"
                }
            }
        }
    }
}
