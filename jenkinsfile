pipeline {
    agent any
    tools {
        // Use the correct Maven version configured in Jenkins
        maven 'Maven'
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Build') {
            steps {
                script {
                    // Use the configured Maven installation
                    bat "mvn clean package"
                }
            }
        }
    }
}
